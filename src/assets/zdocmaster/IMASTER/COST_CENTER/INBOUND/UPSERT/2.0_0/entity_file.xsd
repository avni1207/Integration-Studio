	targetNamespace="http://www.zycus.com/integration/schemas/IntegrationInbound"
	version="1.0">
	<xs:element name="IntegrationDetails"
		type="integration-inbound:inboundIntegrationDetails" />
	<xs:element name="IntegrationEntity"
		type="integration-inbound:inboundIntegrationEntity" />
	<xs:complexType name="inboundIntegrationEntity">
		<xs:sequence>
			<xs:element name="integrationEntityHeader"
				type="integration-inbound:InboundIntegrationHeader" />
			<xs:element name="integrationEntityDetails"
				type="integration-inbound:inboundIntegrationEntityDetails" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InboundIntegrationHeader">
		<!-- <xs:complexContent> -->
		<!-- <xs:extension base="integration-inbound:BaseIntegrationHeader"> -->
		<xs:all>
			<xs:element name="integrationTrackingNumber" type="xs:string" />
			<xs:element minOccurs="1" name="referenceCodeForEntity"
				type="xs:string" />
			<xs:element minOccurs="0" name="additionalInfo" type="xs:string" />
			<xs:element name="action" type="xs:string" />
			<xs:element name="entity" type="xs:string" />
			<xs:element name="source" type="xs:string" />
			<xs:element name="zDocBaseVersion" type="xs:string" />
			<xs:element name="zDocCustomVersion" type="xs:string" />
			<xs:element minOccurs="0" name="timestampUTC" type="xs:string" />
		</xs:all>
		<!-- </xs:extension> -->
		<!-- </xs:complexContent> -->
	</xs:complexType>
	
	<xs:complexType name="COSMAS01">
			<xs:sequence>
				<xs:element name="IDOC">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="EDI_DC40">
