									<xs:documentation>IDoc Control Record for Interface to
										External System</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="TABNAM" type="xs:string"
											fixed="EDI_DC40">
											<xs:annotation>
												<xs:documentation>Name of Table Structure
												</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="MANDT" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Client</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="3" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="DOCNUM" minOccurs="0">
											<xs:annotation>
												<xs:documentation>IDoc number</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="16" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="DOCREL" minOccurs="0">
											<xs:annotation>
												<xs:documentation>SAP Release for IDoc</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="4" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="STATUS" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Status of IDoc</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="2" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="DIRECT">
											<xs:annotation>
												<xs:documentation>Direction</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="1">
														<xs:annotation>
															<xs:documentation>Outbound</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="2">
														<xs:annotation>
															<xs:documentation>Inbound</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="OUTMOD" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Output mode</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="1" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="EXPRSS" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Overriding in inbound processing
												</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="1" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="TEST" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Test flag</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="1" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="IDOCTYP" type="xs:string"
											fixed="COSMAS01">
											<xs:annotation>
												<xs:documentation>Name of basic type</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CIMTYP" minOccurs="0" type="xs:string"
											fixed="">
											<xs:annotation>
												<xs:documentation>Extension (defined by customer)
												</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="MESTYP" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Message type</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
