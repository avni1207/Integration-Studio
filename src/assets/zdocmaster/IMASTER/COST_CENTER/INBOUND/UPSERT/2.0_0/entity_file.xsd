											</xs:simpleType>
										</xs:element>
										<xs:element name="CRETIM" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Created at</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="6" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="REFINT" minOccurs="0">
