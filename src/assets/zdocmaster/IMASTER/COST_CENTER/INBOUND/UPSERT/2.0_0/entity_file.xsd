											</xs:simpleType>
										</xs:element>
										<xs:element name="MESCOD" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Message code</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="3" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="MESFCT" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Message Function</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="3" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="STD" minOccurs="0">
											<xs:annotation>
												<xs:documentation>EDI standard, flag</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="1" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="STDVRS" minOccurs="0">
